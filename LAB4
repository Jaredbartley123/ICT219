clementine


#question 2

version: '3.3'

services:
        mysql:
                image: mysql
                restart: always
                environment:
                        MYSQL_USER: 'jared'
                        MYSQL_ROOT_PASSWORD: 'password'
                ports:
                        - '3307:3306'
                volumes:
                        - ./db:/var/lib/mysql

         postgresql:
                image: postgres
                restart: always
                environment:
                        PORTGRES_USER: 'jared'
                        POSTGRES_PASSWORD: 'password'
                ports:
                        - 5438:5432
                volumes:
                        - ./postgres-data:/var/lib/postgresql/data
                        - ./sql/create_tables.sql:/docker-entrypoint-initdb.d/create_tables.sql
