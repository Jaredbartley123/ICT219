cat /proc/version
cat /proc/meminfo
cat /proc/meminfo | grep MemAvailable
cat /proc/meminfo | grep Shmem:
cat /proc/ioports | grep keyboard
cat /proc/iomem | grep intel
journalctl -b | grep device > boot.log
who -r
telinit 2
systemctl stop mysql
systemctl start mysql
systemctl status mysql
wall -n "Hello everyone!"
cd /etc/init
vim mysql.conf
#modifying the runlevel variables as following.
#start on runlevel [!2]
#stop on runlevel [!5]
telinit 6

